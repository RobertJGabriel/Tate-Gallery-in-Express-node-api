doctype html
html
  head
    title #{title}
    link(rel='stylesheet', href='/css/bootstrap.css')
    link(rel='stylesheet', href='/fonts/octicons.css')
    
    
    block extra-css
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  body(ng-app="tate" ng-controller="controller" data-ng-init="init()")
    #navbar-header.navbar-collapse.collapse.inverse
      .container-fluid
        .about
          ul#pick_colors
            li
              span#_7F8C8D.colors(ng-click="setColor('#7F8C8D')")
                span(ng-if="theme[0][0]['color'] === '#7F8C8D'")
                  i.mega-octicon.octicon-check
            li
              span#_95A5A6.colors(ng-click="setColor('#95A5A6')")
                span(ng-if="theme[0][0]['color'] === '#95A5A6'")
                  i.mega-octicon.octicon-check
            li
              span#_BDC3C7.colors(ng-click="setColor('#BDC3C7')")
                span(ng-if="theme[0][0]['color'] === '#BDC3C7'")
                  i.mega-octicon.octicon-check
            li
              span#_C0392B.colors(ng-click="setColor('#C0392B')")
                span(ng-if="theme[0][0]['color'] === '#C0392B'")
                  i.mega-octicon.octicon-check
            li
              span#_E74C3C.colors(ng-click="setColor('#E74C3C')")
                span(ng-if="theme[0][0]['color'] === '#E74C3C'")
                  i.mega-octicon.octicon-check
            li
              span#_F39C12.colors(ng-click="setColor('#F39C12')")
                span(ng-if="theme[0][0]['color'] === '#F39C12'")
                  i.mega-octicon.octicon-check
            li
              span#_F1C40F.colors(ng-click="setColor('#F1C40F')")
                span(ng-if="theme[0][0]['color'] === '#F1C40F'")
                  i.mega-octicon.octicon-check
            li
              span#_8E44AD.colors(ng-click="setColor('#8E44AD')")
                span(ng-if="theme[0][0]['color'] === '#8E44AD'")
                  i.mega-octicon.octicon-check
            li
              span#_CA2C68.colors(ng-click="setColor('#CA2C68')")
                span(ng-if="theme[0][0]['color'] === '#CA2C68'")
                  i.mega-octicon.octicon-check
            li
              span#_EA4C88.colors(ng-click="setColor('#EA4C88')")
                span(ng-if="theme[0][0]['color'] === '#EA4C88'")
                  i.mega-octicon.octicon-check
            li
              span#_2C3E50.colors(ng-click="setColor('#2C3E50')")
                span(ng-if="theme[0][0]['color'] === '#2C3E50'")
                  .mega-octicon.octicon-check
        .social
          h4 Menu
          ul.list-unstyled
            li
              a(href='/artworks') Artworks
            li
              a(href='/artists') Artists
            li
              a(href='/') Home
              
    .navbar.navbar-static-top.navbar-dark.bg-inverse(ng-style="themeStyle",style="border-radius:0;")
      .container-fluid
        button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbar-header')
          | â˜°
        a.navbar-brand(href='/') Tate Gallery





    section.jumbotron.text-xs-center(style='z-index: 3;background-color: rgba(000,000,000,0.2) !important;    border-radius: 0px; position: relative;')
      .container
        h1.jumbotron-heading.otto #{heading}
        p.lead.text-muted.otto #{subheading}
          
          
          
    block content
    footer.footer
      .container-fluid
        .row(style='margin-left:0px;margin-right:0px;padding-bottom: 40px;')
          .col-md-8.col-sm-8
            .row
              .col-md-4.col-sm-4
                h4(style='color:white;padding-left: 36px;padding-top: 20px;') Shortcuts
                ul
                  li
                    a(href='/artworks') Artworks
                  li
                    a(href='/artists') Artists
                  li
                    a(href='/') Home
              .col-md-4.col-sm-4
                h4(style='color:white;padding-left: 36px;padding-top: 20px;') Connect
                ul
                  li
                    a(href='https://www.facebook.com/tategallery') Facebook
                  li
                    a(href='https://twitter.com/tate') Twitter
                  li
                    a(href='https://www.youtube.com/user/tate') Youtube
                  li
                    a(href='http://www.tate.org.uk/about/our-work/digital/podcast-directory') Podcasts

              .col-md-4.col-sm-4
                h4(style='color:white;padding-left: 36px;padding-top: 20px;') Support
                ul
                  li
                    a(href='http://www.tate.org.uk/join-support/donate') Donate
                  li
                    a(href='http://www.tate.org.uk/join-support/tate-member-patron') Become a Member
                  li
                    a(href='http://www.tate.org.uk/join-support/corporate-support/corporate-members') Corporate Members
                  li
                    a(href='http://www.tate.org.uk/about/business-services/events-and-hospitality') Corporate and private events

          
          .col-md-4.col-sm-4
            h4(style='color:white;padding-top: 20px;') Find Us
            iframe.img-responsive(frameborder='0', style='border:0', src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2483.277258736186!2d-0.09738094899325851!3d51.508129179535345!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x487604a9c77c6bff%3A0xf4267368010a7363!2sBankside%2C+London+SE1%2C+UK!5e0!3m2!1sen!2sie!4v1456662397598')



  script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js")
  script(src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js")
  script(src="http://v4-alpha.getbootstrap.com/assets/js/vendor/holder.min.js")
  script(src="https://maps.googleapis.com/maps/api/js?callback=initMap" , async, defer)
  script(src='/js/core.js')
  script(src='/js/bootstrap.min.js')


  block extra-scripts